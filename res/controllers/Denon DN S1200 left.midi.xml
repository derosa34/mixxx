                </options>
            </control>
            <control>
                <group>[Channel1]</group>
                <key>DNS1200.loopCallback</key>
                <description>B Button Released</description>
                <status>0x80</status>
                <midino>0x39</midino>
                <options>
                    <script-binding/>
                </options>
            </control>
                <!-- Exit /Reloop -->
            <control>
                <group>[Channel1]</group>
                <key>DNS1200.loopCallback</key>
                <description>Exit / Reloop button pressed</description>
                <status>0x90</status>
                <midino>0x40</midino>
