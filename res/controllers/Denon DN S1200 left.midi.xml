<?xml version='1.0' encoding='utf-8'?>
<MixxxControllerPreset schemaVersion="1" mixxxVersion="2.1.0+">
    <info>
        <name>Denon DN-S1200 Left v3.0</name>
        <author>Christophe Rozale</author>
        <description>1-decks mapping for Denon DN-S1200 controller - Left Side (Channel1)</description>
        <forums></forums>
    </info>
    <controller id="DN-S1200-Left">
        <scriptfiles>
            <file functionprefix="DNS1200" filename="Denon-DN-S1200-scriptsv2.js"/>
        </scriptfiles>
        <controls>
            <!-- Assign Deck 1 to Channel 1 | Consider this is the Denon placed of left side -->
                <!-- Loop In -->
            <control>
                <group>[Channel1]</group>
                <!-- <key>loop_in</key> -->
				<key>DNS1200.loopCallback</key>
                <description>A1 Button Pressed</description>
                <status>0x90</status>
                <midino>0x37</midino>
                <options>
                    <script-binding/>
                </options>
            </control>
            <control>
                <group>[Channel1]</group>
                <key>DNS1200.loopCallback</key>
                <description>A1 Button Released</description>
                <status>0x80</status>
                <midino>0x37</midino>
                <options>
                    <script-binding/>
                </options>
            </control>
                <!-- Loop In -->
            <control>
                <group>[Channel1]</group>
                <key>DNS1200.loopCallback</key>
                <description>A2 Button Pressed</description>
                <status>0x90</status>
                <midino>0x38</midino>
                <options>
                    <script-binding/>
                </options>
            </control>
            <control>
                <group>[Channel1]</group>
                <key>DNS1200.loopCallback</key>
                <description>A2 Button Released</description>
                <status>0x80</status>
                <midino>0x38</midino>
                <options>
                    <script-binding/>
                </options>
            </control>
                <!-- Loop Out -->
            <control>
                <group>[Channel1]</group>
                <key>DNS1200.loopCallback</key>
                <description>B Button Pressed</description>
                <status>0x90</status>
                <midino>0x39</midino>
                <options>
