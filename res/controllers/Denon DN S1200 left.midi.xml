                </options>
            </control>
            <control>
                <group>[Channel1]</group>
                <key>DNS1200.playButton</key> <!-- Let's do nothing when released -->
                <description>PLAY/PAUSE Button Released</description>
                <status>0x80</status>
                <midino>0x43</midino>
                <options>
